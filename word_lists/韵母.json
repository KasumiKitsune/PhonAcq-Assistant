    "groups": [
        {
            "id": 1,
            "items": [
                {
                    "text": "a",
                    "note": "开口元音。口张大，舌位低，舌头居中。",
                    "lang": ""
                },
                {
                    "text": "o",
                    "note": "圆唇元音。舌头后缩，舌根抬高，双唇收圆。",
                    "lang": ""
                },
                {
                    "text": "e",
                    "note": "展唇元音。舌位与 o 相似，但双唇是扁平的，向两边咧开。",
                    "lang": ""
                },
                {
                    "text": "i",
                    "note": "高前元音。舌尖抵下齿背，舌面抬高靠近硬腭，嘴角向两边展开（微笑状）。",
                    "lang": ""
                },
                {
                    "text": "u",
                    "note": "高后圆唇元音。舌根抬高靠近软腭，双唇收圆前突（吹口哨状）。",
                    "lang": ""
                },
                {
                    "text": "ü",
                    "note": "高前圆唇元音。舌位与 i 相同，但嘴唇是圆的（像 u）。",
                    "lang": ""
                }
            ]
        },
        {
            "id": 2,
            "items": [
                {
                    "text": "ai",
                    "note": "从 a 的口型滑向 i 的口型。",
                    "lang": ""
                },
                {
                    "text": "ei",
                    "note": "从 e 的口型滑向 i 的口型。",
                    "lang": ""
                },
                {
                    "text": "ui",
                    "note": "是 u 和 ei 的结合，写作 ui。发音时 e 弱化。",
                    "lang": ""
                },
                {
                    "text": "ao",
                    "note": "从 a 的口型滑向 o 的口型，o 轻而短。",
                    "lang": ""
                },
                {
                    "text": "ou",
                    "note": "从 o 的口型滑向 u 的口型。",
                    "lang": ""
                },
                {
                    "text": "iu",
                    "note": "是 i 和 ou 的结合，写作 iu。发音时 o 弱化。",
                    "lang": ""
                },
                {
                    "text": "ie",
                    "note": "从 i 的口型滑向 ê [ɛ] 的口型。",
                    "lang": ""
                },
                {
                    "text": "üe",
                    "note": "从 ü 的口型滑向 ê [ɛ] 的口型。与 j, q, x 拼时去点。",
                    "lang": ""
                },
                {
                    "text": "er",
                    "note": "看作特殊复韵母，发 e [ə] 的音，同时舌尖向硬腭卷起。",
                    "lang": ""
                }
            ]
        },
        {
            "id": 3,
            "items": [
                {
                    "text": "an",
                    "note": "发 a 后，舌尖上抬抵住上齿龈。",
                    "lang": ""
                },
                {
                    "text": "en",
                    "note": "发 e（此处的e读作[ə]）后，舌尖上抬。",
                    "lang": ""
                },
                {
                    "text": "in",
                    "note": "发 i 后，舌尖上抬。",
                    "lang": ""
                },
                {
                    "text": "un",
                    "note": "是 u 和 en 的结合，写作 un。发音时 e 弱化。",
                    "lang": ""
                },
                {
                    "text": "ün",
                    "note": "发 ü 后，舌尖上抬。与 j, q, x 拼时去点。",
                    "lang": ""
                },
                {
                    "text": "ian",
                    "note": "i + an，但中间的 a 读作 ê [ɛ]。",
                    "lang": ""
                },
                {
                    "text": "uan",
                    "note": "u + an。",
                    "lang": ""
                },
                {
                    "text": "üan",
                    "note": "ü + an，中间的 a 读作 ê [ɛ]。与 j, q, x 拼时去点。",
                    "lang": ""
                }
            ]
        },
        {
            "id": 4,
            "items": [
                {
                    "text": "ang",
                    "note": "发 a 后，舌根后缩抬起。",
                    "lang": ""
                },
                {
                    "text": "eng",
                    "note": "发 e [ə] 后，舌根抬起。",
                    "lang": ""
                },
                {
                    "text": "ing",
                    "note": "发 i 后，舌根抬起。",
                    "lang": ""
                },
                {
                    "text": "ong",
                    "note": "是 u 和 eng 的结合，但发音更像 o + ng，发音时从 u 滑向 ng。",
                    "lang": ""
                },
                {
                    "text": "iang",
                    "note": "i + ang。",
                    "lang": ""
                },
                {
                    "text": "uang",
                    "note": "u + ang。",
                    "lang": ""
                },
                {
                    "text": "ueng",
                    "note": "u + eng。",
                    "lang": ""
                },
                {
                    "text": "iong",
                    "note": "üng。",
                    "lang": ""
                }
            ]
        }
    ]
